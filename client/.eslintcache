rm onSubmit={handleFormSubmit}>\r\n              <input style={{marginRight: \"10px\"}}\r\n                className=\"form-input\"\r\n                placeholder=\"Your email\"\r\n                name=\"email\"\r\n                type=\"email\"\r\n                value={formState.email}\r\n                onChange={handleChange}\r\n              />\r\n              <input style={{marginRight: \"10px\"}}\r\n                className=\"form-input\"\r\n                placeholder=\"******\"\r\n                name=\"password\"\r\n                type=\"password\"\r\n                value={formState.password}\r\n                onChange={handleChange}\r\n              />\r\n              <button\r\n                className=\"btn btn-block btn-primary\"\r\n                style={{ cursor: 'pointer' }}\r\n                type=\"submit\"\r\n              >\r\n                Submit\r\n              </button>\r\n            </form>\r\n          )}\r\n\r\n          {error && (\r\n            <div className=\"my-3 p-3 bg-danger text-white\">\r\n              {error.message}\r\n            </div>\r\n          )}\r\n        </div>\r\n      </div>\r\n    </main>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n","C:\\Users\\dexte\\OneDrive\\Documents\\GitHub\\DnD-Character-Builder\\client\\src\\pages\\Home.js",[],"C:\\Users\\dexte\\OneDrive\\Documents\\GitHub\\DnD-Character-Builder\\client\\src\\components\\Header\\index.js",[],"C:\\Users\\dexte\\OneDrive\\Documents\\GitHub\\DnD-Character-Builder\\client\\src\\utils\\auth.js",[],"C:\\Users\\dexte\\OneDrive\\Documents\\GitHub\\DnD-Character-Builder\\client\\src\\utils\\queries.js",[],"C:\\Users\\dexte\\OneDrive\\Documents\\GitHub\\DnD-Character-Builder\\client\\src\\utils\\mutations.js",[],"C:\\Users\\dexte\\OneDrive\\Documents\\GitHub\\DnD-Character-Builder\\client\\src\\components\\CharacterList\\Characters.js",["54","55"],"C:\\Users\\dexte\\OneDrive\\Documents\\GitHub\\DnD-Character-Builder\\client\\src\\pages\\Profile.js",[],{